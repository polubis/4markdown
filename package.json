    "gatsby-transformer-sharp": "^5.14.0",
    "katex": "^0.16.22",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.0.3",
    "rehype-katex": "^7.0.1",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "rxjs": "^7.8.2",
    "zustand": "^4.4.3"
