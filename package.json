{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "description": "blog",
  "author": "polubis",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "22.17.0",
    "npm": "10.9.0"
  },
  "scripts": {
    "benchmark": "node benchmark.js",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|json)\" --write",
    "lint:fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e:1": "cypress run --spec \"cypress/e2e/creator-sync.cy.ts\"",
    "e2e:all": "cypress run",
    "e2e:check-snapshots": "npm run build && start-server-and-test serve http://localhost:9000 \"cypress run\"",
    "ci:lint": "npm run lint",
    "ci:e2e": "npm run build && start-server-and-test serve http://localhost:9000 \"cypress run --env failOnSnapshotDiff=false\"",
    "ci:units": "npm run test"
  },
  "dependencies": {
