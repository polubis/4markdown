    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.14.0",
    "cypress": "^14.5.0",
