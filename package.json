    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cypress-image-snapshot": "^3.1.9",
    "@types/jest": "^29.5.14",
    "@types/katex": "^0.16.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^22.14.1",
