{
	"name": "blog",
	"version": "1.0.0",
	"private": true,
	"description": "blog",
	"author": "polubis",
	"keywords": [
		"gatsby"
	],
	"engines": {
		"node": "22.17.0",
		"npm": "10.9.2"
	},
	"scripts": {
		"benchmark": "node benchmark.js",
		"develop": "gatsby develop",
		"start": "gatsby develop",
		"build": "npm run clean && gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"lint": "npx @biomejs/biome check",
		"lint:fix": "npx @biomejs/biome check --write",
		"format": "npx @biomejs/biome format --write",
		"typecheck": "tsc --noEmit",
		"test": "jest",
		"test:watch": "jest --watch",
		"e2e:1": "cypress run --spec \"cypress/e2e/docs-display.cy.ts\"",
		"e2e:all": "cypress run",
		"e2e:check-snapshots": "npm run build && start-server-and-test serve http://localhost:9000 \"cypress run\"",
		"ci:lint": "npm run lint",
		"ci:e2e": "npm run build && start-server-and-test serve http://localhost:9000 \"cypress run --env failOnSnapshotDiff=false\"",
		"ci:units": "npm run test",
		"prepare": "husky"
	},
	"dependencies": {
		"@dagrejs/dagre": "^1.1.5",
		"@greenonsoftware/react-kit": "^0.1.0",
		"@tailwindcss/typography": "^0.5.16",
		"@xyflow/react": "^12.8.1",
		"classnames": "^2.5.1",
		"date-fns": "^4.1.0",
		"firebase": "^11.10.0",
		"gatsby": "^5.14.5",
		"gatsby-plugin-manifest": "^5.14.0",
		"gatsby-plugin-mdx": "^5.14.1",
		"gatsby-plugin-sitemap": "^6.14.0",
		"gatsby-source-filesystem": "^5.14.0",
		"katex": "^0.16.22",
		"lodash.debounce": "^4.0.8",
		"lodash.throttle": "^4.1.1",
		"prismjs": "^1.30.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.5.0",
		"react-markdown": "^10.1.0",
		"rehype-katex": "^7.0.1",
		"rehype-sanitize": "^6.0.0",
		"remark-gfm": "^4.0.1",
		"remark-math": "^6.0.0",
		"rxjs": "^7.8.2",
		"zustand": "^4.4.3"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.27.1",
		"@biomejs/biome": "^2.0.6",
		"@greenonsoftware/vibetest": "^0.0.1",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/cypress-image-snapshot": "^3.1.9",
		"@types/jest": "^30.0.0",
		"@types/katex": "^0.16.7",
		"@types/lodash.debounce": "^4.0.9",
		"@types/lodash.throttle": "^4.1.9",
		"@types/node": "^22.14.1",
		"eslint": "^8.50.0",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@types/prismjs": "^1.26.5",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@types/webpack-bundle-analyzer": "^4.7.0",
		"autoprefixer": "^10.4.21",
		"babel-jest": "^30.0.4",
		"babel-preset-gatsby": "^3.14.0",
		"cypress": "^14.5.1",
		"cypress-image-snapshot": "^4.0.1",
		"gatsby-plugin-postcss": "^6.14.0",
		"gatsby-plugin-robots-txt": "^1.8.0",
		"gatsby-plugin-root-import": "^2.0.9",
		"husky": "^9.1.7",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^30.0.4",
		"jest-environment-jsdom": "^30.0.4",
		"prettier": "^3.6.2",
		"start-server-and-test": "^2.0.12",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3",
		"webpack-bundle-analyzer": "^4.10.2"
	}
}
